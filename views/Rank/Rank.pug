doctype html
head
    style.
        table {
            font-family: arial, sans-serif;
            border-collapse: collapse;
            width: 100%;
        }
        td, th {
            border: 1px solid #dddddd;
            text-align: left;
            padding: 8px;
        }
        tr:nth-child(even) {
            background-color: #dddddd;
        }
h2 Ranking Table
table
    tr
        th Ranking
        th Player Name
        th Profession
        th Kills
    each rank in allRank
        tr
            td= rank.serial
            td= rank.name
            td= rank.race
            td= rank.kills
